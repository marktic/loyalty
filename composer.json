{
  "name": "marktic/loyalty",
  "type": "library",
  "license": "MIT",
  "description": "loyalty base objects",
  "keywords": [],
  "homepage": "https://github.com/marktic/loyalty",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Marktic\\Loyalty\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Marktic\\Loyalty\\Tests\\": "tests/src",
      "Marktic\\Loyalty\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": ">=8.2",
    "ext-json": "*",
    "bytic/package-base": "^1.0",
    "bytic/actions": "dev-main|^1.0|^2.0",
    "bytic/controllers": "^1.0|^2.0",
    "bytic/controllers-extra": "^1.0|^2.0",
    "bytic/migrations": "~0.13",
    "bytic/orm": "^0.9|^1.0|^2.0",
    "bytic/orm-filters": "^0.9|^1.0|^2.0",
    "bytic/orm-behaviors": "^0.9|^1.0|^2.0",
    "bytic/event-dispatcher": "^1.0|^2.0",
    "bytic/models-smart-properties": "^1.0|^2.0",
    "bytic/translation": "^0.9|^1.0|^2.0"
  },
  "require-dev": {
    "bytic/view": "^1.0|^2.0",
    "bytic/form": "^1.0|^2.0",
    "bytic/admin-base": "^1.0|^2.0",
    "bytic/phpqatools": "^1.0",
    "bytic/report-generator": "^1.0 || ^2.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  }
}
